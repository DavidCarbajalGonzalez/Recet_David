<?xml version="1.0" encoding="UTF-8"?>

<?import java.net.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane xmlns="http://javafx.com/javafx/17.0.12" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.recetas.controller.LoginController">

    <stylesheets>
        <URL value="@../css/style.css" />
    </stylesheets>

    <children>
        <Label alignment="CENTER" contentDisplay="CENTER" layoutX="103.0" prefHeight="34.0" prefWidth="177.0" text="INICIO DE SESIÓN" />
        <TextField fx:id="fieldNombre" layoutX="100" layoutY="50" promptText="Nombre de usuario" />
        <PasswordField fx:id="fieldContrasena" layoutX="100" layoutY="100" promptText="Contraseña" />
        <Button layoutX="63.0" layoutY="150.0" onAction="#onLoginButtonClick" prefHeight="36.0" prefWidth="126.0" text="Iniciar sesión" />
        <Button fx:id="crearCuenta" layoutX="200" layoutY="150" onAction="#onRegisterButtonClick" text="Crear cuenta" />
    </children>
</AnchorPane>
